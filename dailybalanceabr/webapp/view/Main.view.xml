<mvc:View controllerName="FICO.dailybalanceabr.controller.Main" displayBlock="true" xmlns:core="sap.ui.core" xmlns="sap.m"
	xmlns:layout="sap.ui.layout" xmlns:t="sap.ui.table" xmlns:mvc="sap.ui.core.mvc" xmlns:f="sap.f">
	<Page showHeader="false" showFooter="false" title="{i18n>title}">
		<content>
			<IconTabBar id="idIconTabBar" expanded="{device>/isNoPhone}" class="sapUiNoContentPadding" applyContentPadding="false"
			stretchContentHeight="true">
				<items>
					<IconTabFilter text="{i18n>Process}" key="view1">
						<mvc:XMLView viewName="FICO.dailybalanceabr.view.Process" height="100%"/>
					</IconTabFilter>
					<IconTabFilter text="{i18n>Display}" key="view2">
						<mvc:XMLView viewName="FICO.dailybalanceabr.view.Display" height="100%"/>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
	</Page>
</mvc:View>