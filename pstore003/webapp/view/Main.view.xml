<mvc:View controllerName="pstore003.controller.Main" displayBlock="true" xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">
	<Page showHeader="false" showFooter="false" title="{i18n>title}">
		<content>
			<IconTabBar id="idIconTabBar" expanded="{device>/isNoPhone}" class="sapUiNoContentPadding" applyContentPadding="false" stretchContentHeight="true">
				<items>
					<IconTabFilter text="{i18n>Process}" key="view1">
						<mvc:XMLView viewName="pstore003.view.Process" height="100%"/>
					</IconTabFilter>
					<IconTabFilter text="{i18n>Display}" key="view2">
						<mvc:XMLView viewName="pstore003.view.Display" height="100%"/>
					</IconTabFilter>
				</items>
			</IconTabBar>
		</content>
	</Page>
</mvc:View>